# xbindkeys -k
# Screen brightness
"xbacklight -inc 5%"
	XF86MonBrightnessUp

"xbacklight -dec 5%"
	XF86MonBrightnessDown

# Volume mixer
"amixer -D pulse set Master 2%+"
	XF86AudioRaiseVolume

"amixer -D pulse set Master 2%-"
	XF86AudioLowerVolume

"amixer -D pulse set Master 1+ toggle"
	XF86AudioMute

# Media control
"playerctl play-pause"
	XF86AudioPlay

"playerctl next"
	XF86AudioNext

"playerctl previous"
	XF86AudioPrev

# Screnshot
"flameshot gui"
	Print

